{
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs16.x"
    }
  },
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/db.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/db.js"
    }
  ],
  "env": {
    
  }
}
